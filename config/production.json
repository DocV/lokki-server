{
    // Timestamp removed because Heroku adds it automatically
    "logging": {
        "format": "<{{title}}> {{file}}:{{line}} ({{method}}) {{message}}" 
    },
    "redis": {
        "options": {
            "no_ready_check": true
        }
    },
    "sendEmails": true,
    "locMapConfig": {
        "resetLinkAddress": "http://lokki.herokuapp.com"
    },
    "neverCrash": true,
    "pushNotifications": true
}
